<h1>Listing people</h1>

<p><a href="<%= new_person_path %>">Add person</a>
 
<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
  </tr>
 
  <% @people.each do |person| %>
    <tr>
      <td><%= person.first_name %></td>
      <td><%= person.last_name %></td>
      <td><%= link_to 'Show', person_path(person) %></td>
    </tr>
  <% end %>
</table>
